{
  "rules": {
    "indent": [
      true,
      "spaces"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-bound-class-methods"
    ],
    "quotemark": [
      true,
      "double",
      "jsx-double",
      "avoid-escape"
    ],
    "no-trailing-whitespace": true,
    "no-null-keyword": false,
    "no-var-keyword": true,
    "prefer-const": true
  }
}
